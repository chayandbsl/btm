{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/shared-metadata.js",
    "app/(admin)/admin/(auth)/login/layout.jsx",
    "lib/utils.js",
    "app/(admin)/admin/(auth)/_components/form/FacebookLogin.jsx",
    "app/(admin)/admin/(auth)/_components/form/GoogleLogin.jsx",
    "app/(admin)/admin/(auth)/_components/form/Registration.jsx",
    "app/(admin)/admin/(auth)/_components/InfoSection.jsx",
    "app/(admin)/admin/(auth)/_components/SectionHead.jsx",
    "app/(admin)/admin/(auth)/_components/TrustMetrics.jsx",
    "app/(admin)/admin/(auth)/login/google/callback/page.jsx",
    "app/(admin)/admin/(auth)/login/page.jsx",
    "app/(admin)/admin/(auth)/registration/page.jsx",
    "lib/auth/password.js",
    "lib/auth/session.js",
    "lib/auth/validation.js",
    "lib/constants/menuItem.jsx",
    "lib/db.js",
    "lib/types.js",
    "actions/auth/index.js",
    "actions/auth/social.js",
    "prisma/seed.js",
    "prisma/users.js",
    "app/(admin)/admin/dashboard/page.jsx",
    "lib/logger/index.js",
    "lib/logger/utils.js",
    "lib/logger/middleware.js",
    "contexts/auth/SessionContext.jsx",
    "components/layouts/admin/_components/NotificationDropdown.jsx",
    "components/layouts/admin/_components/MessageDropdown.jsx",
    "components/layouts/admin/app-header.jsx",
    "components/layouts/admin/app-sidebar.jsx",
    "config/auth.config.js",
    "config/index.js",
    "config/mail.config.js",
    "hooks/useDropdown.js",
    "contexts/SidebarContext.jsx",
    "lib/auth/JWT.js",
    "lib/auth/cookie.js",
    "app/(admin)/admin/layout.js",
    "app/(admin)/admin/(dashboard)/dashboard/error.jsx"
, "app/not-found.jsx"  ],
  "exclude": ["node_modules"]
}
